{
  "extends": "@lucasecdb",
  "root": true,
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  },
  "parser": "@typescript-eslint/parser",
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "@typescript-eslint/ban-ts-comment": "off",
    "@typescript-eslint/no-var-requires": "off",
    "react/no-unescaped-entities": "off",
    "require-atomic-updates": "off",
    "no-console": "off"
  },
  "settings": {
    "react": {
      "version": "16.8"
    }
  }
}
