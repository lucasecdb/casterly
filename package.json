{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,graphql,gql}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@lucasecdb/eslint-config": "^0.15.1",
    "@types/expect-puppeteer": "^4.4.5",
    "@types/get-port": "^4.2.0",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.17",
    "@types/puppeteer": "^5.4.2",
    "babel-jest": "^26.6.3",
    "eslint": "^7.24.0",
    "get-port": "^5.1.1",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "jest-puppeteer": "^4.4.0",
    "lerna": "^4.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.1.1",
    "puppeteer": "^5.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "typescript": "^4.2.4"
  },
  "resolutions": {
    "@typescript-eslint/eslint-plugin": "~4.8.0"
  },
  "scripts": {
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "dev": "yarn lerna run dev --parallel",
    "test": "jest"
  }
}
