    "prepublishOnly": "yarn build"
  },
  "bugs": {
    "url": "https://github.com/lucasecdb/casterly/issues"
  },
  "peerDependencies": {
    "react": "^16.9.0 || ^17.0.1",
    "react-dom": "^16.9.0 || ^17.0.1",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0"
  }
