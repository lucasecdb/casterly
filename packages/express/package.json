{
  "name": "@casterly/express",
  "version": "0.7.0",
  "description": "Casterly bindings for Express.js",
  "author": "Lucas Cordeiro <ecdb.lucas@gmail.com>",
  "homepage": "https://github.com/lucasecdb/casterly#readme",
  "license": "MIT",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "*.js",
    "*.d.ts"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucasecdb/casterly.git",
    "directory": "packages/express"
  },
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1",
    "clean": "rimraf lib",
    "ts": "tsc",
    "dev": "tsc --watch --preserveWatchOutput",
    "build": "yarn clean && yarn ts",
    "prepublishOnly": "yarn build"
  },
  "bugs": {
    "url": "https://github.com/lucasecdb/casterly/issues"
  },
  "peerDependencies": {
    "@casterly/components": "^0.4.0",
    "express": "^4.17.1",
    "react": "^16.9.0 || ^17.0.0",
    "react-dom": "^16.9.0 || ^17.0.0",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "express": "^4.17.1",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@casterly/core": "^0.7.0"
  }
}
